<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.student.models.Branch" table="branch">
        
        <id name="branchId" column="branch_id">
            <generator class="increment"/>
        </id>

        <property name="branchName" column="branch_name" not-null="true"/>

        <set name="students" table="student_branch" cascade="all-delete-orphan" fetch="select">
            <key column="branch_id"/>
            <one-to-many class="com.student.models.Student"/>
        </set>

    </class>
</hibernate-mapping>
